#: 05040201.xhp#bm_id3150868.help.text
msgid ""
"\\<bookmark_value\\>scaling; axes\\</bookmark_value\\>\\<bookmark_value"
"\\>logarithmic scaling along axes\\</bookmark_value\\>\\<bookmark_value"
"\\>charts;scaling axes\\</bookmark_value\\>\\<bookmark_value\\>X axes;scaling"
"\\</bookmark_value\\>\\<bookmark_value\\>Y axes; scaling\\</bookmark_value\\>"
"\\<bookmark_value\\>axes;interval marks\\</bookmark_value\\>"
msgstr ""
"\\<bookmark_value\\>merilo; osi\\</bookmark_value\\>\\<bookmark_value\\>logarithmično merilo na oseh\\</bookmark_value\\>\\<bookmark_value\\>grafikoni;merilo osi\\</bookmark_value\\>"
"\\<bookmark_value\\>os X;merilo\\</bookmark_value\\>\\<bookmark_value\\>os Y; merilo\\</bookmark_value\\>\\<bookmark_value\\>osi;oznake intervala\\</bookmark_value\\>"

