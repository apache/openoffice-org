msgstr ""

#: 04060107.xhp#par_id3163362.165.help.text
msgid "\\<emph\\>Array 1, Array 2...Array 30\\</emph\\> represent arrays whose corresponding elements are to be multiplied."
msgstr ""

#: 04060107.xhp#par_idN11B19.help.text
msgid "At least one array must be part of the argument list. If only one array is given, all array elements are summed."
msgstr ""

#: 04060107.xhp#par_idN11BA1.help.text
msgid "=SUMPRODUCT(A1:B3;C1:D3) returns 397."
msgstr ""

#: 04060107.xhp#par_idN11BA4.help.text
msgid "Calculation: A1*C1 + B1*D1 + A2*C2 + B2*D2 + A3*C3 + B3*D3"
