msgid "\\<image id=\\\"img_id3159413\\\" src=\\\"res/commandimagelist/sc_drawselect.png\\\" width=\\\"5.64mm\\\" height=\\\"5.64mm\\\"\\>\\<alt id=\\\"alt_id3159413\\\"\\>Icon\\</alt\\>\\</image\\>"
