"\\<variable id=\\\"configure_overview\\\"\\>\\<link href=\\\"text/shared/guide/configure_overview.xhp\\\" name=\\\"Configuring $[officename]\\\"\\>Configuring $[officename]\\</link\\>\\</variable\\>"
