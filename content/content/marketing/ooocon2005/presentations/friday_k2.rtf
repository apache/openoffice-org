\par {\listtext\pard\plain\s15 \f3\lang2057\langfe1033\cgrid0\langnp2057\langfenp1033\insrsid14581316\charrsid12715207 \loch\af3\dbch\af0\hich\f3 \'b7\tab}}\pard\plain \s15\qj \fi-360\li360\ri0\sl280\slmult1\nowidctlpar
