<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://purl.org/NET/xbiblio/csl" xmlns:cs="http://purl.org/NET/xbiblio/csl">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <xs:group name="Classes">
    <xs:choice>
      <xs:group ref="cs:CitationStyle.author-year"/>
      <xs:group ref="cs:CitationStyle.note-bib"/>
      <xs:group ref="cs:CitationStyle.note-nobib"/>
      <xs:group ref="cs:CitationStyle.number"/>
      <xs:group ref="cs:CitationStyle.citekey"/>
    </xs:choice>
  </xs:group>
  <xs:group name="CitationStyle.author-year">
    <xs:annotation>
      <xs:documentation>in-text author-year citations like (Doe, 1999)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="citationstyle">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="cs:info"/>
            <xs:group ref="cs:Content.author-year"/>
          </xs:sequence>
          <xs:attribute name="class" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="author-year"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="CitationStyle.note-bib">
    <xs:annotation>
      <xs:documentation>foot/endnote citations, with a bibliography list</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="citationstyle">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="cs:info"/>
            <xs:group ref="cs:Content.note-bib"/>
          </xs:sequence>
          <xs:attribute name="class" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="note-bib"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="CitationStyle.note-nobib">
    <xs:annotation>
      <xs:documentation>foot/endnote citations, without a bibliography list</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="citationstyle">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="cs:info"/>
            <xs:group ref="cs:Content.note-nobib"/>
          </xs:sequence>
          <xs:attribute name="class" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="note-nobib"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="CitationStyle.number">
    <xs:annotation>
      <xs:documentation>numeric citations like [1] </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="citationstyle">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="cs:info"/>
            <xs:group ref="cs:Content.number"/>
          </xs:sequence>
          <xs:attribute name="class" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="number"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="CitationStyle.citekey">
    <xs:annotation>
      <xs:documentation>citekey in-text citation like [Doe99a]</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="citationstyle">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="cs:info"/>
            <xs:group ref="cs:Content.citekey"/>
          </xs:sequence>
          <xs:attribute name="class" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="citekey"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="info">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="cs:Info.Title"/>
        <xs:element minOccurs="0" ref="cs:title-short"/>
        <xs:element minOccurs="0" ref="cs:edition"/>
        <xs:element minOccurs="0" ref="cs:version"/>
        <xs:element minOccurs="0" ref="cs:author"/>
        <xs:element ref="cs:dateCreated"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="cs:dateModified"/>
        <xs:element minOccurs="0" ref="cs:sources"/>
        <xs:element minOccurs="0" ref="cs:basedOn"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="cs:field"/>
        <xs:element minOccurs="0" ref="cs:description"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:group name="Info.Title">
    <xs:annotation>
      <xs:documentation>The title for the citation style.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="title" type="xs:string"/>
    </xs:sequence>
  </xs:group>
  <xs:element name="title-short" type="xs:token"/>
  <xs:element name="edition" type="xs:string"/>
  <xs:element name="version" type="xs:string"/>
  <xs:element name="author">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="cs:name"/>
        <xs:group minOccurs="0" ref="cs:Info.Url"/>
        <xs:element minOccurs="0" ref="cs:email"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="name" type="xs:string"/>
  <xs:group name="Info.Url">
    <xs:sequence>
      <xs:element name="url" type="xs:anyURI"/>
    </xs:sequence>
  </xs:group>
  <xs:element name="email" type="xs:token"/>
  <xs:element name="dateCreated" type="xs:date"/>
  <xs:element name="dateModified" type="xs:date"/>
  <xs:element name="sources">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="cs:source"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="source">
    <xs:complexType mixed="true">
      <xs:attribute name="type" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="text"/>
            <xs:enumeration value="uri"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="basedOn">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="cs:Info.Title"/>
        <xs:element minOccurs="0" ref="cs:version"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="field" type="xs:token"/>
  <xs:element name="description" type="xs:string"/>
  <xs:group name="Content.author-year">
    <xs:sequence>
      <xs:element name="content">
        <xs:complexType>
          <xs:sequence>
            <xs:group ref="cs:NamesFormat"/>
            <xs:element ref="cs:dates"/>
            <xs:element ref="cs:locators"/>
            <xs:element ref="cs:terms"/>
            <xs:group ref="cs:CitationLayout.author-year"/>
            <xs:group ref="cs:BibliographyLayout.author-year"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="Content.note-bib">
    <xs:sequence>
      <xs:element name="content">
        <xs:complexType>
          <xs:sequence>
            <xs:group ref="cs:NamesFormat"/>
            <xs:element ref="cs:dates"/>
            <xs:element ref="cs:terms"/>
            <xs:group ref="cs:CitationLayout.note-bib"/>
            <xs:group ref="cs:BibliographyLayout.note-bib"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="Content.note-nobib">
    <xs:sequence>
      <xs:element name="content">
        <xs:complexType>
          <xs:sequence>
            <xs:group ref="cs:NamesFormat"/>
            <xs:element ref="cs:dates"/>
            <xs:element ref="cs:terms"/>
            <xs:group ref="cs:CitationLayout.note-nobib"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="Content.number">
    <xs:sequence>
      <xs:element name="content">
        <xs:complexType>
          <xs:sequence>
            <xs:group ref="cs:NamesFormat"/>
            <xs:element ref="cs:dates"/>
            <xs:element ref="cs:terms"/>
            <xs:group ref="cs:CitationLayout.number"/>
            <xs:group ref="cs:BibliographyLayout.number"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="Content.citekey">
    <xs:sequence>
      <xs:element name="content">
        <xs:complexType>
          <xs:sequence>
            <xs:group ref="cs:NamesFormat"/>
            <xs:element ref="cs:dates"/>
            <xs:element ref="cs:terms"/>
            <xs:group ref="cs:CitationLayout.citekey"/>
            <xs:group ref="cs:BibliographyLayout.citekey"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="LayoutStructures">
    <xs:complexContent>
      <xs:extension base="cs:Entry.Biblio"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:attributeGroup name="Citation.Delimiter">
    <xs:attribute name="delimiter" use="required" type="xs:token"/>
  </xs:attributeGroup>
  <xs:group name="CitationLayout.author-year">
    <xs:sequence>
      <xs:element name="citation">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="cs:prefix"/>
            <xs:choice>
              <xs:element ref="cs:all"/>
              <xs:sequence>
                <xs:group ref="cs:FirstReference.author-year"/>
                <xs:group ref="cs:SubsequentReferences.author-year"/>
              </xs:sequence>
            </xs:choice>
            <xs:element minOccurs="0" ref="cs:suffix"/>
          </xs:sequence>
          <xs:attributeGroup ref="cs:Citation.Delimiter"/>
          <xs:attributeGroup ref="cs:Formatting.Attributes"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="all">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="cs:multiple-authors"/>
        <xs:element name="layout" type="cs:CitationEntry.author-year"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:group name="FirstReference.author-year">
    <xs:sequence>
      <xs:element name="first">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="cs:multiple-authors"/>
            <xs:element name="layout" type="cs:CitationEntry.author-year"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="SubsequentReferences.author-year">
    <xs:sequence>
      <xs:element name="subsequent">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="cs:multiple-authors"/>
            <xs:element minOccurs="0" name="layout" type="cs:CitationEntry.author-year"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="CitationEntry.author-year">
    <xs:sequence>
      <xs:element ref="cs:creator"/>
      <xs:element ref="cs:year"/>
      <xs:element ref="cs:point"/>
    </xs:sequence>
    <xs:attributeGroup ref="cs:Formatting.Attributes"/>
  </xs:complexType>
  <!-- FIX ME -->
  <xs:group name="CitationLayout.note-bib">
    <xs:sequence>
      <xs:element name="citation">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="all">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" ref="cs:multiple-authors"/>
                  <xs:group ref="cs:CitationEntry.note-bib"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attributeGroup ref="cs:Citation.Delimiter"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="CitationLayout.note-nobib">
    <xs:sequence>
      <xs:element name="citation" type="cs:CitationEntry.note-nobib"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="CitationEntry.note-bib">
    <xs:sequence>
      <xs:element minOccurs="0" ref="cs:multiple-authors"/>
      <xs:element name="layout">
        <xs:complexType>
          <xs:choice>
            <xs:group ref="cs:Formatting.Elements"/>
            <xs:group ref="cs:Entry.Citation-note"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="CitationEntry.note-nobib">
    <xs:sequence>
      <xs:element name="first">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="cs:multiple-authors"/>
            <xs:element name="layout">
              <xs:complexType>
                <xs:group ref="cs:Entry.Citation-note"/>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="subsequent">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="cs:multiple-authors"/>
            <xs:element minOccurs="0" name="layout">
              <xs:complexType>
                <xs:choice>
                  <xs:group ref="cs:Formatting.Elements"/>
                  <xs:group ref="cs:Entry.Citation-note"/>
                </xs:choice>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="ibid">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="yes"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="cs:Citation.Delimiter"/>
  </xs:complexType>
  <xs:group name="Entry.Citation-note">
    <xs:sequence>
      <xs:group ref="cs:RefClass-Monograph-Required"/>
      <xs:group ref="cs:RefClass-PartInMonograph-Required"/>
      <xs:group ref="cs:RefClass-PartInSerial-Required"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="cs:RefClass-Monograph-Optional"/>
        <xs:group ref="cs:RefClass-PartInMonograph-Optional"/>
        <xs:group ref="cs:RefClass-PartInSerial-Optional"/>
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="CitationLayout.number">
    <xs:sequence>
      <xs:element name="citation">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="cs:prefix"/>
            <xs:element ref="cs:CitationEntry.number"/>
            <xs:element minOccurs="0" ref="cs:suffix"/>
          </xs:sequence>
          <xs:attributeGroup ref="cs:CitationEntry.number"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="CitationEntry.number" abstract="true">
    <xs:complexType/>
  </xs:element>
  <xs:attributeGroup name="CitationEntry.number">
    <xs:attributeGroup ref="cs:Citation.Delimiter"/>
  </xs:attributeGroup>
  <xs:element name="number" substitutionGroup="cs:CitationEntry.number"/>
  <xs:group name="CitationLayout.citekey">
    <xs:sequence>
      <xs:element name="citation">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" ref="cs:prefix"/>
            <xs:element ref="cs:CitationEntry.citekey"/>
            <xs:element minOccurs="0" ref="cs:suffix"/>
          </xs:sequence>
          <xs:attributeGroup ref="cs:CitationEntry.citekey"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="CitationEntry.citekey" abstract="true">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="cs:prefix"/>
        <xs:element minOccurs="0" ref="cs:suffix"/>
      </xs:sequence>
      <xs:attributeGroup ref="cs:Formatting.Attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="CitationEntry.citekey">
    <xs:attributeGroup ref="cs:Citation.Delimiter"/>
  </xs:attributeGroup>
  <xs:element name="point">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="cs:prefix"/>
        <xs:element minOccurs="0" ref="cs:suffix"/>
      </xs:sequence>
      <xs:attributeGroup ref="cs:Formatting.Attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="BibliographyLayout.author-year">
    <xs:sequence>
      <xs:element name="bibliography">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="cs:LayoutStructures">
              <xs:attributeGroup ref="cs:AuthorSort"/>
              <xs:attributeGroup ref="cs:AuthorShorten"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="AuthorSort">
    <xs:attribute name="author-as-sort-order" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="no"/>
          <xs:enumeration value="first-author"/>
          <xs:enumeration value="all"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="AuthorShorten">
    <xs:attribute name="author-shorten">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="yes"/>
          <xs:enumeration value="no"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:group name="BibliographyLayout.note-bib">
    <xs:sequence>
      <xs:element name="bibliography">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="cs:LayoutStructures">
              <xs:attributeGroup ref="cs:AuthorSort"/>
              <xs:attributeGroup ref="cs:AuthorShorten"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="BibliographyLayout.citekey">
    <xs:sequence>
      <xs:element name="bibliography">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="cs:LayoutStructures">
              <xs:attributeGroup ref="cs:AuthorSort"/>
              <xs:attributeGroup ref="cs:AuthorShorten"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="BibliographyLayout.number">
    <xs:sequence>
      <xs:element name="bibliography">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="cs:LayoutStructures">
              <xs:attributeGroup ref="cs:SortOrder-Bib.number"/>
              <xs:attributeGroup ref="cs:AuthorSort"/>
              <xs:attributeGroup ref="cs:AuthorShorten"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="SortOrder-Bib.number">
    <xs:attribute name="sort-order" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="cited"/>
          <xs:enumeration value="author-year"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    By handling this separately from the main citation and 
    bibliography layout, we make the formatting more flexible,
    and the style language easier to handle in GUIs.
  -->
  <xs:element name="terms">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="cs:roles"/>
        <xs:element minOccurs="0" ref="cs:genres"/>
        <xs:element minOccurs="0" ref="cs:media"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:group name="NamesFormat">
    <xs:sequence>
      <xs:element name="names">
        <xs:complexType>
          <xs:attribute name="initialize-with">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value=""/>
                <xs:enumeration value="."/>
                <xs:enumeration value=". "/>
                <xs:enumeration value=" "/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="and-as" use="required">
            <xs:annotation>
              <xs:documentation>"and"-as determines whether multiple names use text (and) or a symbol (&amp;)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="roles">
    <xs:complexType>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="cs:Role.Term"/>
    </xs:complexType>
  </xs:element>
  <!--
    internationalization: should representation of months be handled
    in the schema?
  -->
  <xs:element name="dates" type="cs:Months"/>
  <xs:complexType name="Months">
    <xs:sequence>
      <xs:element ref="cs:months"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="months">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="cs:January"/>
        <xs:group ref="cs:February"/>
        <xs:group ref="cs:March"/>
        <xs:group ref="cs:April"/>
        <xs:group ref="cs:May"/>
        <xs:group ref="cs:June"/>
        <xs:group ref="cs:July"/>
        <xs:group ref="cs:August"/>
        <xs:group ref="cs:September"/>
        <xs:group ref="cs:October"/>
        <xs:group ref="cs:November"/>
        <xs:group ref="cs:December"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:group name="January">
    <xs:sequence>
      <xs:element name="month">
        <xs:complexType mixed="true">
          <xs:attribute name="number" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:integer">
                <xs:enumeration value="01"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="February">
    <xs:sequence>
      <xs:element name="month">
        <xs:complexType mixed="true">
          <xs:attribute name="number" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:integer">
                <xs:enumeration value="02"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="March">
    <xs:sequence>
      <xs:element name="month">
