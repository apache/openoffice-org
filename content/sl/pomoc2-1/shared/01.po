msgstr "\\<image id=\\\"img_id3156005\\\" src=\\\"res/commandimagelist/sc_polygon.png\\\" width=\\\"0.222inch\\\" height=\\\"0.222inch\\\"\\>\\<alt id=\\\"alt_id3156005\\\"\\>Ikona\\</alt\\>\\</image\\>"

