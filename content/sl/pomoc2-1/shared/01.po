msgid ""
"_: 01160300.xhp#hd_id3152425.10.help.text\n"
"File name"
msgstr "Ime datoteke"

#: 01160300.xhp#hd_id3147291.8.help.text
msgid "Current style"
msgstr "Trenutni slog"

#: 01160300.xhp#par_id3156426.7.help.text
msgid "\\<ahelp hid=\\\"HID_FILESAVE_TEMPLATE\\\"\\>Select the paragraph style that you want to use to separate the source document into sub-documents.\\</ahelp\\> The default paragraph style is \"Heading 1\"."
msgstr "\\<ahelp hid=\\\"HID_FILESAVE_TEMPLATE\\\"\\>Izberite slog odstavka, ki ga želite uporabiti za ločevanje glavnega dokumenta v poddokumente.\\</ahelp\\> Privzeti slog odstavka je \"Naslov 1\"."

#: 01160300.xhp#hd_id3153311.11.help.text
msgid ""
"_: 01160300.xhp#hd_id3153311.11.help.text\n"
"File type"
msgstr "Vrsta datoteke"

